mainmenu "Network Wi-Fi station mode application"

config WIFI_SAMPLE_SSID
	string "SSID of the target AP"
	help
	  SSID of the access point to connect to in station mode.
	  For example: "TARGET-AP"

config WIFI_SAMPLE_PSK
	string "Pre-shared key for target AP"
	help
	  WPA2 password used to connect to the target AP.
	  For example: "PSK"

config UDP_SERVER_ADDR
	string "UDP Server IP address"
	help
	  The UDP server IP address
	  For example: "192.168.50.1"

config UDP_SERVER_PORT
	int "UDP Server Port"
	help
	  The UDP server port
	  For example: 4242

source "Kconfig.zephyr"
